{
    "extends": ["airbnb", "react-app"],
    "env": {
        "browser": true,
        "jest": true
    },
    "rules": {
        "comma-dangle": ["error", "never"],
        "semi": ["warn", "never"],
        "react/jsx-filename-extension": "off",
        "react/prop-types": "off",
        "import/prefer-default-export": "off",
        "linebreak-style": ["warn", "windows"]
    }
}
